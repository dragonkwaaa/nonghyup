<script>

// :: 비밀번호 변경 팝업 오픈 스크립트
$(document).on('click', '.changeOptionBtn', function(){
	$('.popup.changeOptionPop').show();
	$('.container').addClass('overlay');
});

// :: 팝업 닫기 스크립트(공용)
$(document).on('click', '.closePopBtn', function(){
	$('.popup').hide();
	$('.container').removeClass('overlay');
});

// :: 셀렉트박스 스크립트(공용)
$(document).on('click', '.sbox a', function(){
	$(this).siblings('ul').slideToggle();
})


// :: ksg : 페이지 상단 대형 이미지 슬라이드 (웹 버전)
$('.mainImgSlider.webViewGroup').slick({
	dots:true,
	prevArrow:false,
	nextArrow:false,
	autoplay : true,
	autoplaySpeed : 55000,
});


// :: ksg :  페이지 상단 대형 이미지 슬라이드 (모바일 버전)
$('.mainImgSlider.mobileViewGroup').slick({
	dots:true,
	prevArrow:false,
	nextArrow:false,
	autoplay : true,
	autoplaySpeed : 55000,
	dotsClass: 'custom_paging',
	customPaging: function (slider, i) {
        console.log(slider);
        return  (i + 1) + '/' + slider.slideCount;
    }
});

</script>

